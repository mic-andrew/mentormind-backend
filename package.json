{
  "name": "mentormind-backend",
  "version": "1.0.0",
  "description": "MentorMind Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "seed": "tsx src/seeds/coachesSeedV2.ts",
    "seed:avatars": "tsx src/seeds/avatarsSeed.ts",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint src/ --ext .ts",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint src/ --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage"
  },
  "dependencies": {
    "@deepgram/sdk": "^3.5.0",
    "apple-signin-auth": "^1.7.6",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.21.1",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.77.0",
    "resend": "^6.9.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "api",
    "backend",
    "oauth",
    "google",
    "apple"
  ],
  "author": "",
  "license": "MIT"
}
